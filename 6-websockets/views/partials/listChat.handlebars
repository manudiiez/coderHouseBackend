<div id="chatContainer">
    {{#if isEmpty}}
    {{#each productos}}
    <div class="chatMessage">
        <div>
            <p class="chatEmail">{{this.email}}</p>
            <p class="chatDate">{{this.date}}</p>
        </div>
        <p class="chatText">{{this.text}}</p>
    </div>
    {{/each}}
    {{else}}
    <div class="alert alert-danger" role="alert">
        No hay Mensajes
    </div>
    {{/if}}

</div>